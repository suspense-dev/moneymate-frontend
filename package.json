{
  "private": true,
  "name": "@moneymate/frontend",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo web",
    "eject": "expo eject",
    "build:ios": "expo build:ios -t simulator",
    "build:android": "expo build:android -t simulator",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write \"./src/**/*.{ts,tsx,js,json}\"",
    "precommit": "lint-staged",
    "typecheck": "yarn tsc --project tsconfig.json --noEmit",
    "commit": "git-cz"
  },
  "dependencies": {
    "@react-native-community/masked-view": "0.1.10",
    "@react-navigation/bottom-tabs": "^5.9.2",
    "@react-navigation/native": "^6.0.10",
    "@react-navigation/native-stack": "^6.6.2",
    "@react-navigation/stack": "^5.9.3",
    "bignumber.js": "^9.0.2",
    "date-fns": "^2.16.1",
    "expo": "^44.0.0",
    "expo-font": "~10.0.4",
    "expo-haptics": "~11.1.0",
    "lodash": "^4.17.20",
    "mobx": "^6.0.1",
    "mobx-react": "^7.0.0",
    "nanoid": "^3.1.12",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-native": "^0.64.3",
    "react-native-gesture-handler": "~2.1.0",
    "react-native-paper": "^4.2.0",
    "react-native-safe-area-context": "3.3.2",
    "react-native-screens": "~3.10.1",
    "react-native-status-bar-height": "^2.5.0",
    "react-native-web": "^0.17.6",
    "styled-components": "^5.2.1"
  },
  "devDependencies": {
    "@babel/core": "7.9.0",
    "@types/lodash": "^4.14.162",
    "@types/nanoid": "^2.1.0",
    "@types/react": "~16.9.49",
    "@types/react-dom": "~16.9.8",
    "@types/react-native": "~0.63.20",
    "@types/styled-components-react-native": "^5.1.1",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-preset-react-native": "^4.0.1",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "7.23.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "3.4.0",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "lint-staged": "10.5.4",
    "prettier": "^2.3.1",
    "typescript": "~3.8.3"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
